generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid()) // esse @id quer dizer que Ã© a chave primaria, e o resto vai gerar um id automatica unico
  username String @unique
  name     String

  created_at DateTime @default(now()) // vai preencher com o valor atual da hora

  @@map("users") // aqui nos colocamos o nome da tabela
}
   